<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mapgps">

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/> //GPS를 이용한 상세 위치 파악 권한
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"/> //기지국, 와이파이를 이용한 대략적 위치 파악 권한

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <meta-data
            android:name="com.naver.maps.map.CLIENT_ID"
            android:value="6krwgkf8rd"/>

        <activity android:name=".MainActivity"
            android:screenOrientation="portrait">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>